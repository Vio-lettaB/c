#include <stdio.h>
int i;
int k;
int n;
    
 int main( )
{
    printf(" введите число N\n" );
    scanf(" %d", &n);
    while(n < 2)
    {
       printf(" N не простое ");
    }
    for(i = 2; i <= n; i++)
    {
        for(k =2; i % k != 0; k++);
        {
            if( k == i)
            {  
                 printf(" %d", i);
            }
        }
    }
}


